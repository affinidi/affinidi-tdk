{
  "name": "@affinidi/tdk-auth-provider",
  "version": "0.1.0",
  "description": "Internal module for managing access tokens",
  "author": "Affinidi",
  "repository": {
    "type": "git",
    "url": "https://github.com/GIT_USER_ID/GIT_REPO_ID.git"
  },
  "keywords": [
    "affinidi-tdk",
    "identity",
    "holistic",
    "network",
    "trust"
  ],
  "license": "Apache-2.0",
  "main": "index.js",
  "typings": "index.d.ts",
  "sideEffects": false,
  "homepage": "https://github.com/affinidi/affinidi-tdk#readme",
  "jsii": {
    "outdir": "dist",
    "versionFormat": "short",
    "targets": {
      "python": {
        "distName": "affinidi-tdk.auth-provider",
        "module": "affinidi_tdk.auth_provider"
      }
    }
  },
  "scripts": {
    "build": "jsii -vvv",
    "build:watch": "jsii -w",
    "lint": "eslint . --ext .ts",
    "package": "jsii-pacmak -vvv",
    "prepare": "npm run build"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "jsonwebtoken": "^9.0.2",
    "qs": "^6.11.2"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.3",
    "@types/qs": "^6.9.8",
    "jsii": "^5.3.25",
    "jsii-pacmak": "^1.95.0",
    "typescript": "^5.2.2"
  },
  "stability": "stable",
  "types": "index.d.ts",
  "bundleDependencies": [
    "axios",
    "jsonwebtoken",
    "qs"
  ]
}
