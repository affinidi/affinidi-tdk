{
  "name": "@affinidi-tdk/iam-client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "iam/iam-client",
  "projectType": "library",
  "private": false,
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "src/clients/typescript/iam-client/dist",
        "main": "src/clients/typescript/iam-client/index.ts",
        "tsConfig": "src/clients/typescript/iam-client/tsconfig.esm.json",
        "assets": ["src/clients/typescript/iam-client/*.md", "src/clients/typescript/iam-client/docs/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "semantic-release": {
      "executor": "@theunderscorer/nx-semantic-release:semantic-release",
      "options": {
        "changelog": true,
        "github": true,
        "npm": true,
        "repositoryUrl": "git@github.com:affinidi/affinidi-tdk.git",
        "branches": ["main"],
        "plugins": [
          [
            "@semantic-release/npm",
            {
              "npmPublish": true
            }
          ]
        ]
      }
    }
  },
  "tags": []
}
