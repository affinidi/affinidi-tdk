{
  "name": "@affinidi/client-iam",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "iam/client-iam",
  "projectType": "library",
  "private": false,
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/iam/client-iam",
        "main": "iam/client-iam/index.ts",
        "tsConfig": "iam/client-iam/tsconfig.lib.json",
        "assets": ["iam/client-iam/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "semantic-release": {
      "executor": "@theunderscorer/nx-semantic-release:semantic-release",
      "options": {
        "changelog": true,
        "github": true,
        "npm": true,
        "repositoryUrl": "git@github.com:affinidi/affinidi-tdk.git",
        "branches": ["main"],
        "plugins": [
          [
            "@semantic-release/npm",
            {
              "npmPublish": true
            }
          ]
        ]
      }
    }
  },
  "tags": []
}
