  "keywords": [
    "openapitools",
    "openapi-generator",
    "openapi",
    "php",
    "sdk",
    "rest",
    "api"
  ],
  "homepage": "https://github.com/affinidi/affinidi-tdk",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Affinidi",
      "homepage": "https://github.com/affinidi/affinidi-tdk"
    }
